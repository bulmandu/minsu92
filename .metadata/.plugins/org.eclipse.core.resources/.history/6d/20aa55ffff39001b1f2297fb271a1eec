package test3;

public class FactorialMain {

	public static void main(String[] args) {
		Factorial factorial = new Factorial();
		try {
			int result = factorial.calculate(1000000);
		}catch(Exception e) {
			e.getMessage();
			System.out.println("StackOverflowError");
		}
		System.out.println(result);
	}

}
